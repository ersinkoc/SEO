{
  "name": "@oxog/seo",
  "version": "1.0.0",
  "description": "Zero-dependency, AI-enhanced on-page SEO scoring engine",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "seo": "./bin/seo.cjs"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./plugins": {
      "import": {
        "types": "./dist/plugins/plugin.interface.d.ts",
        "default": "./dist/plugins/plugin.interface.js"
      },
      "require": {
        "types": "./dist/plugins/plugin.interface.d.cts",
        "default": "./dist/plugins/plugin.interface.cjs"
      }
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "examples:run": "vitest run tests/examples-smoke.test.ts",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build && npm run test:coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ersinkoc/seo.git"
  },
  "homepage": "https://seo.oxog.dev",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  }
}
